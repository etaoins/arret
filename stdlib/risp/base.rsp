(import [risp internal primitives])
(export def defn fn if quote export ... _ defmacro macro-rules)

(export let)
(defmacro let (macro-rules #{} [
  [(let ([name val] ...) expr ...)
   ((fn (name ...) expr ...) val ...)]
]))

(defmacro defn (macro-rules #{} [
  [(defn destruc fn-data ...) (def destruc (fn fn-data ...))]
]))
