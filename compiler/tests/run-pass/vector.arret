(import [stdlib base])
(import [stdlib test])

; TODO: These need to be evaluated at compile time because the compiler can't codegen vectors
; yet.

(defn test-vector! () ->! ()
  (assert-eq! [] (vector))
  (assert-eq! [1 2 3] (vector 1 2 3)))

(defn test-vector-ref! () ->! ()
  (assert-eq! 1 (vector-ref [1 2 3] 0)))

(defn main! () ->! ()
  (test-vector!)
  (test-vector-ref!))