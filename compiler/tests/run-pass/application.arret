(import [arret base])

(defn return-rest (x ...) x)
(def [_ : (Listof Any)] (return-rest 1 2 3))

(defn take-exactly-three (_ _ _) false)
(def [_ : false] (take-exactly-three 1 2 3))
(def [_ : false] (take-exactly-three '(1 2 3) ...))
(def [_ : false] (take-exactly-three 1 2 '(3) ...))

(defn polymorphic-identity-function #{T} ([x : T]) x)
(def [_ : Str] (polymorphic-identity-function "Hello polymorphism!"))